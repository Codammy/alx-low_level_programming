CC = gcc
SRC = main.c school.c
OBJ = $(SRC:.c=.o)
NAME = school
RM = rm

all: $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

%.o: $(SRC)

%.c:
	$(CC) -c $@

clean:
	-$(RM) *~ .*.swp .*.swo $(NAME)

oclean:
	-$(RM) $(OBJ)

fclean:
	-$(RM) *~ .*.swp .*.swo $(NAME)
	-$(RM) $(OBJ)

#re:
#	$(CC) -c $(SRC)
#	$(CC) $(OBJ) -o $(NAME)
